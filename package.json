{
    "name": "node-red-contrib-lasertag",
    "description": "Set of flows to create your own Raspberry Pi lasertag game using Node-RED.",
    "version": "0.0.1",
    "dependencies": {
        "node-red-contrib-oled": "0.2.1",
        "node-red-node-pi-gpio": "1.2.3",
        "node-red-node-serialport": "1.0.1"
    },
    "node-red": {
        "settings": {
            "flowFile": "flows/flows.json",
            "credentialsFile": "flows/flows_cred.json"
        }
    }
}